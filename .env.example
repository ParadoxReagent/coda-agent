# Database
# Note: When running via Docker, DATABASE_URL is overridden automatically
# to use the postgres container. This value is used for local development only.
DATABASE_URL=postgresql://coda:coda@localhost:5432/coda
POSTGRES_PASSWORD=coda

# Redis
# Note: When running via Docker, REDIS_URL is overridden automatically
# to use the redis container. This value is used for local development only.
REDIS_URL=redis://localhost:6379

# Discord
DISCORD_BOT_TOKEN=
DISCORD_CHANNEL_ID=
DISCORD_ALLOWED_USER_IDS=

# LLM Providers (set in config.yaml or override here)
ANTHROPIC_API_KEY=
GOOGLE_API_KEY=
OPENAI_API_KEY=
OPENROUTER_API_KEY=

# Memory Service
MEMORY_API_KEY=

# Firecrawl (web scraping/crawling) - API v2
# Cloud: set FIRECRAWL_API_KEY (get key from https://firecrawl.dev)
# Self-hosted: set FIRECRAWL_API_URL (e.g., http://localhost:3002)
# See: https://github.com/firecrawl/firecrawl
FIRECRAWL_API_KEY=
FIRECRAWL_API_URL=

# n8n Webhook Service (for inbound events from n8n)
# Generate secret with: openssl rand -base64 32
N8N_WEBHOOK_SECRET=
N8N_WEBHOOK_HEADER_NAME=x-webhook-secret

# REST API Authentication
API_KEY=

# Application
NODE_ENV=development
LOG_LEVEL=info
CONFIG_PATH=./config/config.yaml
